#include<iostream>
#include<cstdio>
#include<cstring>
using namespace std;
int main()
{
    int a,b,c,i,n,d=0,d1=0,d3=0;
    cin>>n;
    for(i=0;i<n;i++)
    {
        cin>>a>>b>>c;
        cout<<endl;
        d=d+a;
        d1=d1+b;
        d3=d3+c;

    }
    if(d==0&&d1==0&&d3==0)
    {
        cout<<"YES"<<endl;
    }
    else cout<<"NO"<<endl;

}
